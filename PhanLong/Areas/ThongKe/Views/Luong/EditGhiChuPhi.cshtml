@model PhanLong.EF.PhatSinhChiThu

<div class="form-group form-group-sm">
    <input type="hidden" readonly name="Id" id="psid" value="@Model.Id" />
    <label>Ghi chú phí:</label>
    <input type="text" name="GhiChu" id="GhiChu" placeholder="Nhập ghi chú phí ..." class="form-control form-control-sm" value="@Model.GhiChu" />
</div>
<div class="form-group form-group-sm">
    <button type="button" class="btn btn-default" data-dismiss="modal">Đóng</button>
    <input type="submit" href="#" class="btn btn-primary" value="Lưu" id="SaveGhiChuchi" />
</div>



